@using And.ElkomMuhasebe.Core.Model;
@using And.ElkomMuhasebe.Core.Model.Entity.Admin;
@using And.ElkomMuhasebe.Core.Model.Entity.FirmaDB;
@model And.ElkomMuhasebe.UI.WEB.Controllers.CARI_BASLIKController.modelimiz

@{
    ViewBag.Title = "Cari Kartı";
}

@using (Html.BeginForm(null, null, new { @id = string.Empty }, FormMethod.Post,
    new { @id = "postform" }))
{
    @Html.AntiForgeryToken()
    @Html.HiddenFor(model => model.Cari.ID)
    @Html.HiddenFor(model => model.Cari.BAKIYE)
    @Html.HiddenFor(model => model.Cari.TOPLAMALACAK)
    @Html.HiddenFor(model => model.Cari.PARABIRIMI)
    @Html.HiddenFor(model => model.Cari.TOPLAMBORC)
    @Html.HiddenFor(model => model.Cari.CreateUserID)
    @Html.HiddenFor(model => model.Cari.CreateDate)
    @Html.HiddenFor(model => model.Cari.DOVIZBAKIYE)
    @Html.HiddenFor(model => model.Cari.TOPLAMDOVIZALACAK)
    @Html.HiddenFor(model => model.Cari.TOPLAMDOVIZBORC)
    @Html.ValidationSummary(true, "", new { @class = "text-danger" })

    <nav>
        <div class="nav nav-tabs" id="nav-tab" role="tablist">
            <a class="nav-link active" id="nav-home-tab" data-toggle="tab" href="#nav-home" role="tab" aria-controls="nav-home" aria-selected="true">Genel</a>
            <a class="nav-link" id="nav-iletisim-tab" data-toggle="tab" href="#nav-iletisim" role="tab" aria-controls="nav-iletisim" aria-selected="false">İletişim</a>
            <a class="nav-link" id="nav-detay-tab" data-toggle="tab" href="#nav-detay" role="tab" aria-controls="nav-detay" aria-selected="false">Detay</a>
            <a class="nav-link" id="nav-banka-bilgileri-tab" data-toggle="tab" href="#nav-banka-bilgileri" role="tab" aria-controls="nav-banka-bilgileri" aria-selected="false">Banka Bilgileri</a>
            <a class="nav-link" id="nav-ozel-fiyat-tab" data-toggle="tab" href="#nav-ozel-fiyat" role="tab" aria-controls="nav-ozel-fiyat" aria-selected="false">Özel Fiyatlar</a>
        </div>
    </nav>
    <div class="tab-content" id="nav-tabContent">
        <!-- Genel -->
        <div class="tab-pane fade show active" id="nav-home" role="tabpanel" aria-labelledby="nav-home-tab">
            <div class="card border-top-0 rounded-0">
                <div class="card-body p-3">
                    <div class="form-row">
                        <div class="form-group col-md-6">
                            <label for="CARIKOD">Cari Kod :</label>
                            <div class="input-group">
                                @Html.EditorFor(model => model.Cari.CARIKOD, new { htmlAttributes = new { @class = "form-control form-control-sm", @placeholder = "Kod otomatik gelsin", @aria_label = "...", @aria_describedby = "button-addon2" } })
                                @Html.ValidationMessageFor(model => model.Cari.CARIKOD, "", new { @class = "text-danger" })
                                <div class="input-group-append">
                                    <button class="btn btn-sm btn-outline-secondary" title="Kod üret" type="button" id="button-addon2"><i class="fas fa-plus"></i></button>
                                </div>
                            </div>
                        </div>
                        <div class="form-group col-md-3">
                            <label for="tipi">Tipi :</label>
                            <select id="Cari_Mükelleftipi" name="Cari.Mükelleftipi" class="form-control form-control-sm">
                                <option selected>@Model.Cari.Mükelleftipi.ToString()</option>
                                <option value="0" >Kurumsal</option>
                                <option value="1">Şahıs</option>
                                <option value="2">Kamu</option>

                                <option value="3">İhracat</option>
                                <option value="4">SGK</option>
                            </select>
                        </div>
                        <div class="form-group col-md-3">

                            <label for="Cari_PARABIRIMI">Döviz :</label>
                            <select name="Cari.PARABIRIMI" id="Cari_PARABIRIMI" readonly class="form-control form-control-sm">
                                <option selected>@Model.Cari.PARABIRIMI</option>
                            </select>

                        </div>

                        <div class="form-group col-md-6">
                            <label for="FIRMAUNVANI">Ünvanı :</label>
                            <div class="input-group">
                                @Html.EditorFor(model => model.Cari.FIRMAUNVANI, new { htmlAttributes = new { @class = "form-control form-control-sm", @placeholder = "..." } })
                                @Html.ValidationMessageFor(model => model.Cari.FIRMAUNVANI, "", new { @class = "text-danger" })
                            </div>
                        </div>

                        <div class="form-group col-md-3">
                            <label for="Cari_ADI">Adı :</label>
                            <div class="input-group">
                                @Html.EditorFor(model => model.Cari.ADI, new { htmlAttributes = new { @class = "form-control form-control-sm", @placeholder = "...", @onchange = "matchunvan()" } })
                                @Html.ValidationMessageFor(model => model.Cari.ADI, "", new { @class = "text-danger" })
                            </div>
                        </div>
                        <div class="form-group col-md-3">
                            <label for="Cari_SOYADI">Soyadı :</label>
                            <div class="input-group">
                                @Html.EditorFor(model => model.Cari.SOYADI, new { htmlAttributes = new { @class = "form-control form-control-sm", @placeholder = "...", @onchange = "matchunvan()" } })
                                @Html.ValidationMessageFor(model => model.Cari.SOYADI, "", new { @class = "text-danger" })
                            </div>
                        </div>


                        <div class="form-group col-md-6">
                            <label for="FIRMAUNVANI">Vergi Dairesi :</label>
                            <select class="form-control form-control-sm select2" id="Cari_VERGIDAIRESI" name="Cari.VERGIDAIRESI">
                                <option selected>@Model.Cari.VERGIDAIRESI</option>
                                @{if (Model.vERGIDAIREs != null)
                                    {
                                        foreach (var item in Model.vERGIDAIREs)
                                        {
                                            <option>@item.ismi</option>
                                        }
                                    }}
                            </select>
                        </div>
                        <div class="form-group col-md-2">
                            <label for="VERGINUMARASI">VKN / TC :</label>
                            @Html.EditorFor(model => model.Cari.VERGINUMARASI, new { htmlAttributes = new { @class = "form-control form-control-sm", @placeholder = "..." } })
                            @Html.ValidationMessageFor(model => model.Cari.VERGINUMARASI, "", new { @class = "text-danger" })
                        </div>

                        <div class="form-group col-md-4">
                            <label for="gibpk">GİB PK Adresi :</label>
                            <div class="input-group">
                                @Html.EditorFor(model => model.Cari.gibpk, new { htmlAttributes = new { @class = "form-control form-control-sm", @placeholder = "...", @aria_label = "...", @aria_describedby = "button-gibpk" } })
                                @Html.ValidationMessageFor(model => model.Cari.gibpk, "", new { @class = "text-danger" })
                                <div class="input-group-append">
                                    <button class="btn btn-sm  btn-outline-secondary" onclick="mukellefsorgula()" title="PK Sorgula" type="button" id="button-gibpk"><i class="fas fa-search"></i></button>
                                </div>
                            </div>
                        </div>

                        <div class="form-group col-md-12">
                            <label for="ISADRES1">Adres :</label>
                            @Html.EditorFor(model => model.Cari.ISADRES1, new { htmlAttributes = new { @class = "form-control form-control-sm", @placeholder = "..." } })
                            @Html.ValidationMessageFor(model => model.Cari.ISADRES1, "", new { @class = "text-danger" })
                        </div>
                        <div class="form-group col-md-3">
                            <label for="Cari_ULKE">Ülke :</label>
                            <select class="form-control form-control-sm select2" id="Cari_ULKE" name="Cari.ULKE">
                                <option value="@Model.Cari.ULKE" selected>@Model.Cari.ULKE</option>
                                @{


                                    if (Model.ULKELERs != null)
                                    {
                                        foreach (var item in Model.ULKELERs)
                                        {


                                            <option value="@item.KODU">@item.ADI</option>


                                        }
                                    }
                                }
                            </select>
                        </div>
                        <div class="form-group col-md-3">
                            <label for="Cari_ISIL">İl :</label>
                            <select class="form-control form-control-sm select2" id="Cari_ISIL" name="Cari.ISIL">
                                <option selected>@Model.Cari.ISIL</option>
                                @{
                                    ILILCE itemb = new ILILCE();

                                    if (Model.ILILCEs != null)
                                    {
                                        foreach (var item in Model.ILILCEs)
                                        {
                                            if (itemb.iladi != item.iladi)
                                            {
                                                itemb.iladi = item.iladi;
                                                <option>@item.iladi</option>
                                            }

                                        }
                                    }
                                }
                            </select>
                        </div>
                        <div class="form-group col-md-3">
                            <label for="Cari_ISILCE">İlçe :</label>
                            <select class="form-control form-control-sm select2" id="Cari_ISILCE" name="Cari.ISILCE">
                                <option selected="selected">@Model.Cari.ISILCE</option>
                            </select>
                        </div>
                        <div class="form-group col-md-2">
                            <label for="ISTEL">Telefon :</label>
                            <input type="text" class="form-control" id="Cari_ISTEL" name="Cari.ISTEL" value="@Model.Cari.ISTEL" data-inputmask='"mask": "(999) 999-9999"' data-mask>
                            @Html.ValidationMessageFor(model => model.Cari.ISTEL, "", new { @class = "text-danger" })
                        </div>
                        <div class="form-group col-md-2">
                            <label for="MOBIL">Mobil :</label>
                            <input type="text" class="form-control" id="Cari_MOBIL" name="Cari.MOBIL" value="@Model.Cari.MOBIL" data-inputmask='"mask": "(999) 999-9999"' data-mask>

                            @Html.ValidationMessageFor(model => model.Cari.MOBIL, "", new { @class = "text-danger" })
                        </div>
                        <div class="form-group col-md-3">
                            <label for="WEB_SITE">Website :</label>
                            @Html.EditorFor(model => model.Cari.WEB_SITE, new { htmlAttributes = new { @class = "form-control form-control-sm" } })
                            @Html.ValidationMessageFor(model => model.Cari.WEB_SITE, "", new { @class = "text-danger" })
                        </div>
                        <div class="form-group col-md-3">
                            <label for="E_POSTA">E-Posta :</label>
                            @Html.EditorFor(model => model.Cari.E_POSTA, new { htmlAttributes = new { @class = "form-control form-control-sm" } })
                            @Html.ValidationMessageFor(model => model.Cari.E_POSTA, "", new { @class = "text-danger" })
                        </div>
                        <div class="form-group col-md-3">
                            <label for="">TL Bakiye :</label>
                            @Html.EditorFor(model => model.Cari.BAKIYE, new { htmlAttributes = new { @class = "form-control form-control-sm", @readonly = "true" } })
                            @Html.ValidationMessageFor(model => model.Cari.BAKIYE, "", new { @class = "text-danger" })
                        </div>
                        <div class="form-group col-md-3">
                            <label for="">Döviz Bakiye :</label>
                            @Html.EditorFor(model => model.Cari.DOVIZBAKIYE, new { htmlAttributes = new { @class = "form-control form-control-sm", @readonly = "true" } })
                            @Html.ValidationMessageFor(model => model.Cari.DOVIZBAKIYE, "", new { @class = "text-danger" })
                        </div>
                        <div class="form-group col-md-2">
                            <label for="Cari_AKTIF">Durumu :</label>
                            @if (Model.Cari.AKTIF == 1)
                            {
                                <div class="form-check switch switch-lg success full-width right m-b-0">
                                    <input type="checkbox" value="1" id="Cari_AKTIF" name="Cari.AKTIF" checked>
                                    <label for="Cari_AKTIF">Aktif</label>
                                </div>}
                            else
                            { <div class="form-check switch switch-lg success full-width right m-b-0">
                                    <input type="checkbox" value="1" id="Cari_AKTIF" name="Cari.AKTIF">
                                    <label for="Cari_AKTIF">Aktif</label>
                                </div>

                            }
                        </div>

                    </div>

                </div>
            </div>
        </div>
        <!-- İletişim -->
        <div class="tab-pane fade" id="nav-iletisim" role="tabpanel" aria-labelledby="nav-iletisim-tab">
            <div class="card border-top-0 rounded-0">
                <div class="card-body p-3">
                    <div class="form-row">



                    </div>
                </div>
            </div>
        </div>
        <!-- Detay -->
        <div class="tab-pane fade" id="nav-detay" role="tabpanel" aria-labelledby="nav-detay-tab">
            <div class="card border-top-0 rounded-0">
                <div class="card-body p-3">
                    <div class="form-row">
                        <div class="form-group col-md-3">
                            <label for="">Kredi :</label>
                            @Html.EditorFor(model => model.Cari.KREDI, new { htmlAttributes = new { @class = "form-control form-control-sm" } })
                            @Html.ValidationMessageFor(model => model.Cari.KREDI, "", new { @class = "text-danger" })
                        </div>
                        <div class="form-group col-md-3">
                            <label for="">Risk :</label>
                            @Html.EditorFor(model => model.Cari.RISK, new { htmlAttributes = new { @class = "form-control form-control-sm" } })
                            @Html.ValidationMessageFor(model => model.Cari.RISK, "", new { @class = "text-danger" })
                        </div>
                        <div class="form-group col-md-3">
                            <label for="">Vade Opsiyon (Gün) :</label>
                            @Html.EditorFor(model => model.Cari.VADEOPSIYON, new { htmlAttributes = new { @class = "form-control form-control-sm" } })
                            @Html.ValidationMessageFor(model => model.Cari.VADEOPSIYON, "", new { @class = "text-danger" })
                        </div>
                        <div class="form-group col-md-3">
                            <label for="">Vade Farkı (Aylık) :</label>
                            @Html.EditorFor(model => model.Cari.VADE_FARKI, new { htmlAttributes = new { @class = "form-control form-control-sm" } })
                            @Html.ValidationMessageFor(model => model.Cari.VADE_FARKI, "", new { @class = "text-danger" })
                        </div>
                        <div class="form-group col-md-3">
                            <label for="">Yetkili Adı Soyadı :</label>
                            @Html.EditorFor(model => model.Cari.MUHATAPADI, new { htmlAttributes = new { @class = "form-control form-control-sm" } })
                            @Html.ValidationMessageFor(model => model.Cari.MUHATAPADI, "", new { @class = "text-danger" })
                        </div>
                        <div class="form-group col-md-3">
                            <label for="">Alış İskonto :</label>
                            @Html.EditorFor(model => model.Cari.ALISISKONTO, new { htmlAttributes = new { @class = "form-control form-control-sm" } })
                            @Html.ValidationMessageFor(model => model.Cari.ALISISKONTO, "", new { @class = "text-danger" })
                        </div>
                        <div class="form-group col-md-3">
                            <label for="">Satış İskonto :</label>
                            @Html.EditorFor(model => model.Cari.SATISISKONTO, new { htmlAttributes = new { @class = "form-control form-control-sm" } })
                            @Html.ValidationMessageFor(model => model.Cari.SATISISKONTO, "", new { @class = "text-danger" })
                        </div>
                       

                        <div class="form-group col-md-3">
                            <label for="Cari_KOD1">Kategori 1 :</label>
                            <div class="input-group input-group-sm">
                                <select class="custom-select" id="Cari_KOD1" name="Cari.KOD1">
                                    <option selected>@Model.Cari.KOD1</option>
                                    @{

                                        if (Model.cARI_KOD1s != null)
                                        {
                                            foreach (var item in Model.cARI_KOD1s)
                                            {

                                                <option>@item.KOD</option>

                                            }
                                        }}
                                </select>
                                <div class="input-group-append">
                                    <button class="btn btn-sm btn-outline-secondary" type="button" data-toggle="modal" data-target="#ModalOZELKOD1"><i class="fas fa-plus"></i></button>
                                </div>
                            </div>
                        </div>
                        <div class="form-group col-md-3">
                            <label for="Cari_KOD2">Kategori 2 :</label>
                            <div class="input-group input-group-sm">
                                <select class="custom-select " id="Cari_KOD2" name="Cari.KOD2">
                                    @{
                                        <option selected>@Model.Cari.KOD2</option>

                                        if (Model.cARI_KOD2s != null)
                                        {
                                            foreach (var item in Model.cARI_KOD2s)
                                            {

                                                <option>@item.KOD</option>

                                            }
                                        }}
                                </select>
                                <div class="input-group-append">
                                    <button class="btn btn-sm btn-outline-secondary" type="button" data-toggle="modal" data-target="#ModalOZELKOD2"><i class="fas fa-plus"></i></button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- Banka -->
        <div class="tab-pane fade" id="nav-banka-bilgileri" role="tabpanel" aria-labelledby="nav-banka-bilgileri-tab">
            <div class="card border-top-0 rounded-0">
                <div class="card-body p-3">
                    <div class="form-row">
                        .... banka-bilgileri
                    </div>
                </div>
            </div>
        </div>
        <!-- Özel Fiyatlar -->
        <div class="tab-pane fade" id="nav-ozel-fiyat" role="tabpanel" aria-labelledby="nav-ozel-fiyat-tab">
            <div class="card border-top-0 rounded-0">
                <div class="card-body p-3">
                    <div class="form-row">
                        <div class="table-responsive">
                            <table id="bootstrap-table" class="table">
                                <thead>
                                    <tr>
                                        <th data-field="STOKADI" data-sortable="true">Stok Kodu/Adı</th>
                                        <th data-field="FIYAT" data-sortable="true">Fiyat</th>
                                        <th data-field="ISK" data-sortable="true">İsk. Oran</th>
                                        <th width="8%&qucot;"></th>
                                    </tr>
                                    <tr>
                                        <td>
                                            <div class="input-group input-group-sm ">
                                                <input type="text" class="form-control form-control-sm" name="Stok" id="Stok" onchange="birimgetir(),tutargetir()" placeholder="Sıtok Adı" aria-describedby="button-stok-sec">
                                                <div class="input-group-append">
                                                    <button class="btn btn-sm btn-outline-secondary" type="button" id="button-stok-sec"
                                                            onclick="stoklistele()" data-toggle="modal" data-target="#ModalStokSec">
                                                        <i class="fa fa-search" aria-hidden="true"></i>
                                                    </button>
                                                </div>
                                            </div>
                                        </td>
                                        <td>
                                            <div class="input-group input-group-sm ">
                                                <input type="text" class="form-control form-control-sm" name="Fiyat" id="Fiyat" placeholder="Fiyat" aria-describedby="stok-fiyat">
                                                <div class="input-group-append">
                                                    <span class="input-group-text" id="stok-fiyat">₺</span>
                                                </div>
                                            </div>
                                        </td>
                                        <td>
                                            <div class="input-group input-group-sm ">
                                                <input type="text" class="form-control form-control-sm" name="Iskonto" id="Iskonto" placeholder="İskonto Oranı" aria-describedby="stok-iskonto-orani">
                                                <div class="input-group-append">
                                                    <span class="input-group-text" id="stok-iskonto-orani">%</span>
                                                </div>
                                            </div>
                                        </td>
                                        <td>
                                            <button type="button" id="addRowBtn" onclick="butonlistele()"  class="btn btn-secondary  btn-sm btn-rounded">
                                                <span class="icon"><i class="fas fa-plus"></i></span>
                                            </button>
                                        </td>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model.CARI_FIYATs)
                                    {
                                    <td>@item.STOKKOD</td>
                                    <td>@item.FIYAT</td>
                                    <td>@item.ORAN</td>
                                    <td> <button type="button" id="deleteRowBtn" onclick="deleteRow(this)" class="btn btn-danger btn-sm btn-rounded">  <span class="icon text-white-50">    <i class="fas fa-trash-alt"></i> </span></button></td>
                                }
                                    </tbody>
                                </table>
                                <button type="button" class="btn btn-outline-primary btn-sm" onclick="butongonder()">Özel Fiyat Kaydet</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>


        <div class="py-2">
            <button type="button" class="btn btn-sm btn-primary btn-icon-split" onclick="datagonder()">
                <span class="icon text-white-50">
                    <i class="fas fa-save"></i>
                </span>
                <span class="text">Kaydet</span>

            </button>
            <a href="@Session["History"]" class="btn btn-sm btn-secondary btn-icon-split">
                <span class="icon text-white-50">
                    <i class="fas fa-power-off"></i>
                </span>
                <span class="text">İptal</span>
            </a>
        </div>



    }

<div class="modal fade" id="ModalOZELKOD1" tabindex="-1" aria-labelledby="ModalOZELKOD1Label" aria-hidden="true">
    <div class="modal-dialog modal-xl">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="ModalOZELKOD1Label">Yeni Cari Özel Kod 1</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="form-group">
                    <label for="özelkod1">Başlık</label>
                    <input type="text" class="form-control" id="özelkod1" name="özelkod1">
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-outline-dark" data-dismiss="modal">İptal</button>
                <button type="button" class="btn btn-primary" onclick="özelkodolustur1()">Kaydet</button>
            </div>
        </div>
    </div>
</div>

<div class="modal fade" id="ModalOZELKOD2" tabindex="-1" aria-labelledby="ModalOZELKOD2Label" aria-hidden="true">
    <div class="modal-dialog modal-xl">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="ModalOZELKOD2Label">Yeni Cari Özel Kod 2</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="form-group">
                    <label for="özelkod2">Başlık</label>
                    <input type="text" class="form-control" id="özelkod2" name="özelkod2">
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-outline-dark" data-dismiss="modal">İptal</button>
                <button type="button" class="btn btn-primary" onclick="özelkodolustur2()">Kaydet</button>
            </div>
        </div>
    </div>
</div>


<div class="modal fade" id="ModalStokSec" tabindex="-1" role="dialog" aria-labelledby="ModalStokSecLabel" aria-hidden="true">
    <div class="vertical-alignment-helper">
        <div class="modal-dialog modal-xl modal-xl">
            <div class="modal-content">
                <div class="modal-header">
                    <h4 class="modal-title" id="ModalStokSecLabel">Stok Seç</h4>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">×</span></button>
                </div>
