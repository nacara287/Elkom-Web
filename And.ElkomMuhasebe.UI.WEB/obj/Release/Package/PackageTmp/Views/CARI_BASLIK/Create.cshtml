            function tutargetir() {
                var kur = parseFloat(document.getElementById("Hareket_KUR").value.replaceAll(",",""));

                var s1 = parseFloat(document.getElementById("Hareket_ALACAK").value.replaceAll(",", "")) * kur;
                var s2 = parseFloat(document.getElementById("Hareket_BORC").value.replaceAll(",", "")) * kur;
                var toplam = s2 - s1;
                document.getElementById("Hareket_BAKIYE").value = toplam.toString();

            }





