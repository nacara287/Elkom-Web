            var childs = rowid.parentNode.parentNode.childNodes;

            var fiyat = 0;
            var miktar = 0;
            var isk = 0;
            var kur = 0;
            var nettutar = null;
            var kdvtutar = null;
            var kdvtd = null;
            for (var i = 0; i < childs.length; i++) {
                if (childs[i].id == "tdfiyat") {

                    fiyat = parseFloat(childs[i].firstElementChild.value.replaceAll(",",""))
                }
                if (childs[i].id == "tdmiktar") {
